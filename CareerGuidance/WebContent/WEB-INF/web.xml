		<servlet-name>ModifyInstituteServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.ModifyInstituteController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ModifyInstituteServlet</servlet-name>
		<url-pattern>/modifyinstitute</url-pattern>
	</servlet-mapping>
</web-app>  
    
    
  