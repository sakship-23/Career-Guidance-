<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>loginclient.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>CourseServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.CourseController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CourseServlet</servlet-name>
		<url-pattern>/course</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>InstituteServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.InstituteController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InstituteServlet</servlet-name>
		<url-pattern>/institute</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LocationServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.LocationController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LocationServlet</servlet-name>
		<url-pattern>/location</url-pattern>
	</servlet-mapping>

<servlet>
