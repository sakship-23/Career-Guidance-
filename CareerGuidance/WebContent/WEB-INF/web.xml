	</servlet>
	<servlet-mapping>
		<servlet-name>AddCourseServlet</servlet-name>
		<url-pattern>/addcourse</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteCourseServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.DeleteCourseController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteCourseServlet</servlet-name>
		<url-pattern>/deletecourse</url-pattern>
	</servlet-mapping>


<servlet>
		<servlet-name>ModifyCourseServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.ModifyCourseController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ModifyCourseServlet</servlet-name>
		<url-pattern>/modifycourse</url-pattern>
	</servlet-mapping>

<servlet>
		<servlet-name>AddLocationServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.AddLocationController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddLocationServlet</servlet-name>
		<url-pattern>/addlocation</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteLocationServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.DeleteLocationController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteLocationServlet</servlet-name>
		<url-pattern>/deletelocation</url-pattern>
	</servlet-mapping>


<servlet>
		<servlet-name>ModifyLocationServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.ModifyLocationController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ModifyLocationServlet</servlet-name>
		<url-pattern>/modifylocation</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddInstituteServlet</servlet-name>
		<servlet-class>com.careerguidance.user.controller.AddInstituteController</servlet-class>
